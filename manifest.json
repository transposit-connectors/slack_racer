{
  "v2": {
    "transposit": {
      "dependencies": [{
        "name": "transposit/airtable:v2.*.*",
        "alias": "airtable"
      }, {
        "name": "transposit/slack:v3.*.*",
        "alias": "slack"
      }],
      "operations": [{
        "id": "get_random_paragraph",
        "function": {
          "js": "get_random_paragraph.js",
          "lang": "js"
        }
      }, {
        "id": "webhook_finished",
        "function": {
          "parameters": [{
            "name": "http_event",
            "default": {
              "text": "{\"http_method\": \"GET\", \"query_parameters\": {}, \"headers\": {}, \"body\": \"\"}"
            },
            "type": "OBJECT"
          }],
          "js": "webhook_finished.js",
          "lang": "js"
        },
        "access": {
          "requireApiKey": true,
          "webhook": true
        }
      }, {
        "id": "generate_diff",
        "function": {
          "parameters": [{
            "name": "input",
            "type": "STRING",
            "required": true
          }, {
            "name": "original",
            "type": "STRING",
            "required": true
          }],
          "js": "untitled.js",
          "lang": "js"
        }
      }, {
        "id": "get_highscores",
        "function": {
          "js": "get_highscores.js",
          "lang": "js"
        },
        "access": {
          "requireUser": true
        }
      }, {
        "id": "slack_post_chat_ephemeral",
        "tql": {
          "parameters": [{
            "name": "userId",
            "type": "STRING"
          }, {
            "name": "channelId",
            "type": "STRING"
          }, {
            "name": "text",
            "type": "STRING",
            "required": true
          }],
          "tql": "-- This operation gets run when the user has not yet visited the configuration page to connect their\n-- slack user. Please visit Users >> User Configuration for more details.\nSELECT * FROM slack.post_chat_ephemeral\n  WHERE $body.channel = @slackChannelId\n  AND $body.user = @slackUserId\n  AND $body.text = @text\n  -- AND $body.blocks = (SELECT [{\n  --     \"type\": \"section\",\n  --     \"text\": {\n  --       \"type\": \"mrkdwn\",\n  --       \"text\": \"Please configure your user: https://slack-racer-p0s5c.transposit.io/settings\"\n  --     },\n  --   }])\n"
        }
      }, {
        "id": "webhook_serve_text",
        "function": {
          "parameters": [{
            "name": "http_event",
            "default": {
              "text": "{   \"query_parameters\": {},   \"headers\": {     \"X-Slack-Signature\": \"v0=XXX\",     \"Accept\": \"application/json,*/*\",     \"X-Request-ID\": \"fXXX\",     \"X-Forwarded-Proto\": \"https\",     \"User-Agent\": \"Slackbot 1.0 (+https://api.slack.com/robots)\",     \"Host\": \"api.transposit.com\",     \"Accept-Encoding\": \"gzip,deflate\",     \"X-Forwarded-Port\": \"443\",     \"X-Amzn-Trace-Id\": \"Root=1-5cdec48b-69a949c8591a0c7d9943762d\",     \"X-Slack-Request-Timestamp\": \"1558103179\",     \"X-Forwarded-For\": \"18.209.110.231\",     \"Content-Length\": \"351\",     \"Content-Type\": \"application/x-www-form-urlencoded\"   },   \"http_method\": \"POST\",   \"body\": \"token=XXX&team_id=TXXX&team_domain=transposit&channel_id=DXXX&channel_name=directmessage&user_id=UXXX&user_name=iggy&command=%2Fcommand&text=sampletext&response_url=https%3A%2F%2Fhooks.slack.com%2Fcommands%2FXXX\" }"
            },
            "type": "OBJECT"
          }],
          "js": "webhook.js",
          "lang": "js"
        },
        "access": {
          "requireApiKey": true,
          "webhook": true
        }
      }, {
        "id": "generate_dialog_json",
        "function": {
          "parameters": [{
            "name": "slackBody",
            "type": "OBJECT",
            "required": true
          }],
          "js": "found.js",
          "lang": "js"
        }
      }],
      "dataConnections": [{
        "ref": "slack_identify.raw",
        "requireConsumerSignin": true,
        "overrides": {
          "swagger": {
            "oauthConfig": {
              "scope": "identity.basic"
            },
            "executionLimits": {
            }
          }
        },
        "title": "Slack",
        "description": "Used to identify you on Slack"
      }, {
        "ref": "slack_bot.raw",
        "requireConsumerSignin": true,
        "overrides": {
          "swagger": {
            "oauthConfig": {
              "scope": "chat:write:bot channels:write"
            },
            "executionLimits": {
            }
          }
        }
      }, {
        "ref": "slack.raw",
        "overrides": {
          "swagger": {
            "oauthConfig": {
              "scope": "commands,bot",
              "accessTokenPath": "access_token",
              "enableGroupCredentials": "true"
            },
            "executionLimits": {
            }
          }
        }
      }],
      "javascriptAPIVersion": "3",
      "source": "src",
      "userSettings": [{
        "name": "slack_id",
        "type": "STRING",
        "hidden": true
      }]
    }
  }
}